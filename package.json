{
  "name": "master-admin-test",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:brandi": "NODE_ENV=development APP_ENV=development vite --host ladmin.brandi.co.kr --port 3001 --open http://ladmin.brandi.co.kr:3001",
    "dev:hiver": "NODE_ENV=development APP_ENV=development vite --host ladmin.hiver.co.kr --port 3001 --open http://ladmin.hiver.co.kr:3001",
    "dev:seoulstore": "NODE_ENV=development APP_ENV=development vite --host ladmin.seoulstore.com --port 3001 --open http://ladmin.seoulstore.com:3001",
    "dev:selpi": "NODE_ENV=development APP_ENV=development vite --host ladmin.selpi.co.kr --port 3001 --open http://ladmin.selpi.co.kr:3001",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "APP_ENV=production tsc && vite build --mode production --debug",
    "build:staging": "APP_ENV=staging NODE_ENV=staging tsc && vite build --mode staging --debug",
    "build:sprint": "APP_ENV=sprint NODE_ENV=sprint tsc && vite build --mode sprint --debug",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.51.15",
    "@tanstack/vue-query-devtools": "^5.51.15",
