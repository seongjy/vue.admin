    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
